            "Training": {
                "title": "Training",
                "description": "Un modèle représentant une séance d'entraînement",
                "properties": {
                    "id": {
                        "description": "ID de la séance",
                        "type": "integer",
                        "example": 1
                    },
                    "nom": {
                        "description": "Nom de la séance",
                        "type": "string",
                        "example": "Séance de musculation"
                    },
                    "user_id": {
                        "description": "ID de l'utilisateur propriétaire de la séance",
                        "type": "integer",
                        "example": 1
                    },
                    "exercices": {
                        "description": "Liste des exercices associés à la séance",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Exercice"
                        }
                    },
                    "created_at": {
                        "description": "Date de création de la séance",
