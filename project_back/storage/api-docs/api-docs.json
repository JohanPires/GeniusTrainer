                "properties": {
                    "id": {
                        "description": "ID de l'exercice",
                        "type": "integer",
                        "example": 1
                    },
                    "nom": {
                        "description": "Nom de l'exercice",
                        "type": "string",
                        "example": "Pompes"
                    },
                    "description": {
                        "description": "Description de l'exercice",
                        "type": "string",
                        "example": "Exercice de renforcement musculaire"
                    },
                    "series": {
                        "description": "Nombre de séries",
                        "type": "integer",
                        "example": 3
                    },
                    "repetitions": {
                        "description": "Nombre de répétitions",
                        "type": "integer",
                        "example": 12
                    },
                    "training_id": {
                        "description": "ID de la séance associée à cet exercice",
                        "type": "integer",
                        "example": 1
                    },
                    "created_at": {
                        "description": "Date de création de l'exercice",
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "description": "Date de dernière mise à jour de l'exercice",
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
